                    android:layout_weight="1"
                    android:fontFamily="sans-serif"
                    android:gravity="center_vertical"
                    android:text="@string/TIPO_MUNICION"
                    android:textColor="@color/black"
                    android:textSize="12sp"
                    android:textStyle="normal"></TextView>

                <TextView
